{
  "permissions": {
    "allow": [
      "Bash(curl:*)",
      "Bash(source:*)",
      "Bash(python:*)",
      "Bash(netstat:*)",
      "Bash(kill:*)",
      "Bash(taskkill:*)",
      "Bash(pip install:*)",
      "Bash(find:*)",
      "Bash(grep:*)",
      "Bash(npm run build:*)",
      "Bash(for i in {1..12})",
      "Bash(do echo \"=== Run $i ===\")",
      "Bash(done)",
      "Bash(pkill:*)",
      "Bash(for:*)",
      "Bash(do echo \"=== Poll $i ===\")",
      "Bash(do echo \"Poll $i:\")",
      "Bash(xargs:*)",
      "Bash(wmic process where \"CommandLine like ''%uvicorn%'' and CommandLine like ''%8000%''\" get ProcessId)",
      "Bash(do taskkill //PID $pid //F)",
      "Bash(wmic process where \"name=''python.exe'' and CommandLine like ''%uvicorn%''\" call terminate)",
      "Bash(wmic process where \"name=''python.exe''\" get ProcessId,CommandLine)",
      "Bash(powershell -Command \"Stop-Process -Id 18532 -Force\")",
      "Bash(wmic:*)",
      "Bash(ls:*)",
      "Bash(cat:*)"
    ]
  }
}
